<!DOCTYPE html>
<html>
  <head>
    <title>Upwork Blog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <%= render 'blox/navbar' %>
      <%= render 'blox/jumbo' %>
    </header>

    <div id="main">



      <%= yield %>
    </div>
    <footer>
      <%= render 'blox/footer' %>
    </footer>
  <script src="https://kit.fontawesome.com/8b41eb6f53.js" crossorigin="anonymous"></script>
  </body>
</html>
